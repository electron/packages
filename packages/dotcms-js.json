{
  "name": "dotcms-js",
  "version": "0.0.5",
  "description": "Set of components and services which allow you to connect to use data from a dotCMS installation",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "start": "electron main.js",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "show": "webpack-dev-server --config showcase/config/webpack.dev.js --inline --progress --port 7070",
    "show-build": "rimraf showcase/dist && webpack --config showcase/config/webpack.prod.js --progress --profile --bail",
    "show-deploy": "sh ./publish-to-dotcms.sh",
    "deploy-public": "npm run show-build; npm run build; npm run show-deploy",
    "test": "karma start --single-run"
  },
