    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-electron-launcher": "^0.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha-reporter": "^2.0.3",
    "karma-safari-launcher": "^1.0.0",
    "karma-tap": "^1.0.4",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.2",
    "parallelshell": "^2.0.0",
