      ]
    },
    "nw": {
      "output": "release/FitSize.nw.min.js",
      "source": [
        "lib/FitSize.js"
      ]
    },
    "el": {
      "output": "release/FitSize.el.min.js",
      "source": [
        "lib/FitSize.js"
      ]
    }
  },
